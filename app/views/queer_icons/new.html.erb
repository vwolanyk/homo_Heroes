<h1>Add to the Iconography</h1>
<%= link_to "Homo", queer_icons_path %>

<%=form_for(@icon) do |f| %>

<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>

</p>

<p>
  <%= f.label :quote %><br>
  <%= f.text_area :quote %>

</p>

<p>
  <%= f.label :img_url %><br>
  <%= f.text_field :img_url %>

</p>

<p>
  <%= f.submit "ADD QUEERO" %>
</p>

<% end %>
